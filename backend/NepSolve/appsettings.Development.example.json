{
    // note: put db name after mongodb.net/ and before ?retryWrites
    "ConnectionStrings": {
        "DbConnection": "<connection_string>"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Jwt": {
        "Key": "<256_bit_jwt_secret_key>",
        "Issuer": "nepsolve-webapp",
        "Audience": "nepsolve-webapp"
    },
    "GROQ_API_KEY": "api_key",
    "COHERE_API_KEY": "api_key",
    "BACKEND_BASE_URL": "http://localhost:5131"
}
